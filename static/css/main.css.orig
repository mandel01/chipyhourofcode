/* checkbox */

    .register-box input[type="checkbox"] {
        margin-left: 1em;
        display: none;
    }

    .register-box input[type="checkbox"] + label {
        position: relative;
        display: block;
        padding: 1em 1em 0.8em 1.5em;
        line-height: 1.5em;
        color: #CCCCCC;
        font-size: 1em;
        margin: 0;
        box-sizing: border-box;
    }

        .register-box input[type="checkbox"] + label:hover {
            cursor: pointer;
        }

    .register-box input[type="checkbox"]:checked + label {
        margin: 0;
        background: url('../img/cross.png') no-repeat 1em 1.19em;
        color: #808080;
    }

    .register-box label::before {
        content: '';
        display: inline-block;
        position: relative;
        width: 15px;
        height: 15px;
        right: 0.5em;
        border: 2px solid #E6E6E6;
    }

        .register-box input[type="checkbox"]:checked + label::before {
            border-color: #808080;
        }

