/*
==========================================================================
GENERAL
========================================================================== */
body {
    font-family: "Oxygen", Arial, sans-serif;
    line-height: 3em;
    color: #4D4D4D;
}

h1, h2, h3, h4, h5, h6, a, span, p, input {
    font-family: "Oxygen", Arial, sans-serif;
}

select, select:hover {
    display: inline-block;
    margin: 0;
    background: none;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    border-color: #E6E6E6;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    text-indent: 0.01px;
    -ms-text-overflow: "";
    -o-text-overflow: "";
    text-overflow: "";
}

select:focus {
    outline: none;;
}

section {
    padding: 5em 0;
}

input, input:focus, input[type="text"], input[type="text"]:focus {
    background-color: transparent;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    border: none;
    font-size: 1em;
}

input:focus, input[type="text"]:focus {
    outline: none;
}

::-webkit-input-placeholder {
    color: #CCC;
}

:-moz-placeholder {
    /* Firefox 18- */
    color: #CCC;
}

::-moz-placeholder {
    /* Firefox 19  */
    color: #CCC;
}

:-ms-input-placeholder {
    color: #CCC;
}

ul {
    margin: 0;
}

li {
    list-style-type: none;
}

a {
    color: #42c2d2;
}

a:hover {
    color: #12A2C2;
}

p:last-of-type {
    margin-bottom: 0;
}

::selection {
    background: #91DEE7;
    /* WebKit/Blink Browsers */;
}

::-moz-selection {
    background: #91DEE7;
    /* Gecko Browsers */;
}

.column, .columns, .wrapper {
    padding-left: 2.9375rem;
    padding-right: 2.9375rem;
}

.no-padding {
    padding-left: 0;
    padding-right: 0;
}

.wrapper, .wrapper-column {
    padding-top: 1.5em;
    padding-bottom: 4em;
    overflow: auto;
}

.section-title {
    margin-bottom: 1.5em;
    font-weight: bold;
    color: #fff;
    text-transform: uppercase;
}

.subpage .section-title {
    margin: 1.5em 0 0 0;
    text-transform: uppercase;
    color: #666;
    font-size: 1.3em;
    font-weight: normal;
}

.round-corners {
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
}

.to {
    display: table;
}

.ti {
    display: table-cell;
    vertical-align: middle;
}

#arrow {
    width: 16px;
    height: 16px;
    border-right: 4px solid #fff;
    border-bottom: 4px solid #fff;
    position: absolute;
    margin: 2em 0 3em 0;
    left: 50%;
    margin-left: -15px;
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)";
    filter: alpha(opacity=90);
    opacity: 0.9;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
}

#over {
    position: relative;
    top: 100%;
    z-index: 500;
}

.ghost {
    visibility: hidden;
}


.d3-tip {
    line-height: 1;
    font-weight: bold;
    padding: 12px;
    background: rgba(0, 0, 0, 0.8);
    color: #fff;
    border-radius: 2px;
}

.d3-tip:after {
    box-sizing: border-box;
    display: inline;
    font-size: 10px;
    width: 100%;
    line-height: 1;
    color: rgba(0, 0, 0, 0.8);
    content: "\25BC";
    position: absolute;
    text-align: center;
}

.d3-tip.n:after {
    margin: -1px 0 0 0;
    top: 100%;
    left: 0;
}

/*
==========================================================================
SPLASH
========================================================================== */
#splash {
    width: 100%;
    height: 100%;
    position: fixed;
    background-size: cover;
    background: url(../img/hero.jpg) no-repeat center center
}

#splash-bg {
    width: 100%;
    height: 100%;
    position: fixed;
    -webkit-background-size: cover;
    background-size: cover;
    z-index: -150;
    background: url(../img/hero.jpg) no-repeat center center;
}

#foreign-country {
    display: none;
    background-color: #DD5D5C;
    color: #fff;
}

#overlay {
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
    filter: alpha(opacity=80);
    opacity: 0.8;
    background-color: #000;
    z-index: -50;
}

#splash-video {
    position: fixed;
    right: 0;
    bottom: 0;
    z-index: -100;
    min-width: 100%;
    min-height: 100%;
    width: auto;
    height: auto;
    -webkit-background-size: cover;
    background-size: cover;
    display: none;
}

#splash-return {
    position: fixed;
    right: 1.5em;
    top: 1em;
    display: inline-block;
    padding: 0 1.5em;
    color: white;
    font-weight: bold;
    text-transform: uppercase;
    z-index: 50;
    background-color: #646464;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
    filter: alpha(opacity=60);
    opacity: 0.6;
    -moz-transition: 0.2s opacity;
    -o-transition: 0.2s opacity;
    -webkit-transition: 0.2s opacity;
    transition: 0.2s opacity;
    display: none;
}

#splash-return:hover {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
    filter: alpha(opacity=100);
    opacity: 1;
    -moz-transition: 0.2s opacity;
    -o-transition: 0.2s opacity;
    -webkit-transition: 0.2s opacity;
    transition: 0.2s opacity;
}

#splash-info {
    position: relative;
    top: 0;
    height: 100%;
    color: #fff;
}

#splash .to {
    width: 99%;
    height: 100%;
    position: relative;
    top: -6em;
}

.site-title, .site-title a, .event-date, .event-host, .site-description, #video-link:link, #video-link:visited, .into-link:focus {
    color: #fff;
}

#splash .site-title {
    margin: 0 0 0.3em 0;
}

.site-title {
    display: inline-block;
    margin: 0.6em 0 0.2em 0;
    padding-left: 1em;
    background: url(../img/logo.png) no-repeat left 67%;
    -webkit-background-size: 0.8em;
    background-size: 0.8em;
    font-weight: 400;
    font-size: 4.8em;
    line-height: 1.2em;
}

span.current-city, input.current-city, input.current-city:focus {
    display: inline-block;
    font-weight: normal;
    color: #fff;
    border-bottom: 2px dotted #fff;
    position: relative;
    height: 1.4em;
    line-height: 1.5em;
    text-align: left;
    text-indent: 0;
}

input.current-city {
    display: none;
    margin: 0;
    padding: 0 0 0.2em 0;
}

/* CITY-PICKER PANEL */

.no-click, .no-click:hover {
    cursor: default;
    background: inherit;
}

.site-description, .event-date, .subscription-message {
    line-height: 2em;
}

.event-date a {
    color: #fff;
    text-decoration: underline;
}

.site-description {
    margin-top: 0.5em;
    font-size: 1.4em;
    font-weight: 300;
}

.event-date, .event-host {
    font-size: 1.3em;
    font-weight: 700;
}

.register-button {
    margin-top: 1.2em;
    padding: 1.2em 3em 1.1em 3em;
    background-color: #DD5D5C;
    background-color: #0094CA;
    font-size: 1.2em;
}

.register-button:hover {
    background-color: #D35353;
}

#subscription-container {
    margin: 1.9em auto 0 auto;
}

#subscription-message {
    margin: 1.4em 0 2.1em 0;
    font-size: 1.1em;
}

body .subscription-email, body .subscription-email:focus {
    background: #fff;
    width: 15em;
    height: 2.5em;
    display: inline-block;
    box-shadow: none;
    -webkit-border-top-left-radius: 3px;
    -moz-border-radius-topleft: 3px;
    border-top-left-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    -moz-border-radius-bottomleft: 3px;
    border-bottom-left-radius: 3px;
    font-size: 1.2em;
    box-shadow: none;
    border: none;
}

body .subscription-submit {
    width: 7em;
    display: inline-block;
    height: 2.5em;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topright: 3px;
    border-top-right-radius: 3px;
    -webkit-border-bottom-right-radius: 3px;
    -moz-border-radius-bottomright: 3px;
    border-bottom-right-radius: 3px;
    background: #DD5D5C;
    font-size: 1.2em;
    -moz-transition: 0.2s background;
    -o-transition: 0.2s background;
    -webkit-transition: 0.2s background;
    transition: 0.2s background;
    position: relative;
}

#video-link:link {
    display: inline-block;
    margin-top: 0.5em;
    padding: 0.8em 0 0.8em 2.3em;
    font-size: 1.2em;
    background: url(../img/video-icon.png) no-repeat left center;
}

/*
==========================================================================
NAVIGATION
========================================================================== */
#nav-button {
    margin-top: 0.5em;
    padding-left: 2.5em;
    display: inline-block;
    z-index: 500;
    background: url(../img/nav-icon.png) no-repeat 1em 50%;
    color: #fff;
    text-transform: uppercase;
    border: none;
    font-weight: 700;
    -webkit-background-size: 0.9em;
    background-size: 0.9em;
}

.subpage #nav-button {
    display: none;
}

.nav-mobile, .nav-large {
    position: relative;
    z-index: 50;
}

.site-nav {
    padding-top: 0.55em;
}

.site-nav ul {
    white-space: nowrap;
    display: none;
    float: right;
}

.subpage .site-nav ul {
    display: block;
}

.site-nav li {
    display: inline-block;
    margin: 1.2em 3em 0 0;
}

.site-nav li:last-of-type {
    margin-right: 2em;
}

.subpage .site-nav li:last-of-type {
    margin-right: 0;
}

.site-nav a {
    text-transform: uppercase;
    color: #fff;
    font-weight: 600;
}

.site-nav a:hover, .site-nav .current-page {
    border-bottom: 1px solid #fff;
}

.current-page {
    pointer-events: none;
}

/*
==========================================================================
FAQ
========================================================================== */
#faq {
    position: relative;
    background-color: #7665a0;
    z-index: 100;
    color: #fff;
    -moz-box-shadow: 0 -10px 15px 0 rgba(50, 50, 50, 0.4);
    -webkit-box-shadow: 0 -10px 15px 0 rgba(50, 50, 50, 0.4);
    box-shadow: 0 -10px 15px 0 rgba(50, 50, 50, 0.4);
}

#faq .row {
    margin-bottom: 2.5em;
}

.fadein {
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
    opacity: 0;
}

.faq-q {
    color: #fff;
    font-size: 1.5em;
    font-weight: normal;
    line-height: 1.5em;
}

.faq-a a {
    color: #ffffff;
    text-decoration: underline;
}

/*
==========================================================================
SCHEDULE
========================================================================== */
#schedule {
    background-color: #83BAC6;
}

.schedule-panel {
    background-color: #fff;
}

.schedule-panel ul {
    padding: 1em 2em;
}

.schedule-panel li {
    border-bottom: 1px solid #F2F2F2;
}

.schedule-panel li:last-of-type {
    border: none;
}

.schedule-day {
    margin: 0;
    padding: 1em 0 0.7em 0;
    font-size: 1.5em;
    color: #fff;
    text-transform: uppercase;
    background-color: #DD5D5C;
    -webkit-border-top-left-radius: 3px;
    -moz-border-radius-topleft: 3px;
    border-top-left-radius: 3px;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topright: 3px;
    border-top-right-radius: 3px;
}

.schedule-time {
    display: inline-block;
    padding: 1.8em 1em 1.5em 0;
    width: 6em;
    vertical-align: top;
    text-align: right;
    color: #B3B3B3;
}

.schedule-task {
    position: relative;
    max-width: 14em;
    margin: 1em 0 1em 1em;
    padding: 0.6em 1em 0.4em 1em;
    display: inline-block;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    color: #666;
    background-color: #F9F9F9;
    -moz-box-shadow: 3px 3px 0 0 #E6E6E6;
    -webkit-box-shadow: 3px 3px 0 0 #E6E6E6;
    box-shadow: 3px 3px 0 0 #E6E6E6;
    -moz-transition: all 0.1s ease;
    -o-transition: all 0.1s ease;
    -webkit-transition: all 0.1s ease;
    transition: all 0.1s ease;
}

.schedule-task:hover {
    position: relative;
    bottom: 3px;
    left: -3px;
    -moz-box-shadow: 5px 5px 0 0 #E6E6E6;
    -webkit-box-shadow: 5px 5px 0 0 #E6E6E6;
    box-shadow: 5px 5px 0 0 #E6E6E6;
    -moz-transition: all 0.1s ease;
    -o-transition: all 0.1s ease;
    -webkit-transition: all 0.1s ease;
    transition: all 0.1s ease;
}

.schedule-task a {
    color: #B1B5B6;
    font-size: 0.8em;
    font-weight: bold;
    text-transform: uppercase;
}

.red-tab, .purple-tab {
    position: absolute;
    display: inline-block;
    width: 5px;
    height: 100%;
    top: 0;
    left: 0;
    -webkit-border-top-left-radius: 3px;
    -moz-border-radius-topleft: 3px;
    border-top-left-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    -moz-border-radius-bottomleft: 3px;
    border-bottom-left-radius: 3px;
    background: #DD5D5C;
}

.purple-tab {
    background: #C3B7D6;
}

.schedule-popup {
    display: none;
    position: absolute;
    right: -15em;
    top: 0;
    width: 12em;
    padding: 0.5em 1em;
    z-index: 100;
    background-color: #F9F9F9;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    -moz-box-shadow: 5px 5px 0 0 #E6E6E6;
    -webkit-box-shadow: 5px 5px 0 0 #E6E6E6;
    box-shadow: 5px 5px 0 0 #E6E6E6;
}

.schedule-popup:before {
    content: "";
    width: 0;
    height: 0;
    border-bottom: 9px solid transparent;
    border-top: 9px solid #F9F9F9;
    border-right: 9px solid transparent;
    border-left: 9px solid transparent;
    font-size: 0;
    line-height: 0;
    position: absolute;
    top: 0;
    left: -9px;
}

/*
==========================================================================
PITCH
========================================================================== */
#pitch {
    position: relative;
    background-color: #FFA400;
    color: #fff;
    font-size: 1.3em;
}

#pitch p {
    font-size: 1em;
    color: #C27515;
}

#pitch .tag {
    font-size: 1.9em;
    color: #fff;
}

#pitch .register-button {
    margin-top: 1em;
    font-size: 1em;
    background-color: transparent;
    border: 2px solid #fff;
}

#pitch .register-button:hover {
    color: #FFAE47;
    border-color: rgba(255, 255, 255, 0.8);
    background: rgba(255, 255, 255, 0.8);
}

#pitch .subscription-email, #pitch .subscription-submit {
    margin-top: 1em;
    font-size: 1em;
}

/*
==========================================================================
SPONSORS LIST
========================================================================== */
#sponsors {
    background-color: #fff;
    overflow: auto;
    padding-bottom: 0;
    position: relative;
    clear: both;
}

#sponsors .section-title {
    color: #4D4D4D;
}

.current-sponsor, .past-sponsor {
    border: 1px #f0f0f0 solid;
    height: 250px;
    float: left;
    text-align: center;
}

.sponsor-image {
    height: 70%;
    width: 70%;
    margin-top: 7%;
    margin-left: 15%;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    background-size: contain;
}

.sponsor-nuit .sponsor-image {
    background-image: url(../img/sponsors/nuit.png);
}

.sponsor-ctd .sponsor-image {
    background-image: url(../img/sponsors/ctd.jpg);
}

.sponsor-chipy .sponsor-image {
    background-image: url(../img/sponsors/chipy.png);
}

.sponsor-programmingforbiologists .sponsor-image {
    background-image: url(../img/sponsors/pfb.png);
}

.past-sponsor {
    height: 200px;
}

/*
==========================================================================
SUBPAGE
========================================================================== */
.header-subpage {
    overflow: auto;
}

.header-subpage .column:first-of-type {
    padding-right: 0;
}

.header-subpage .column:last-of-type {
    padding-left: 0;
}

.subpage {
    /* padding-bottom: 0.1em; */
    background-color: #F2F2F2;
}

.subpage .site-title {
    margin-top: 1.3em;
    padding-left: 1.3em;
    -webkit-background-size: 18%;
    background-size: 18%;
    font-size: 1.3em;
}

.subpage-title {
    margin: 0.5em 0 1.5em 0;
    text-transform: uppercase;
    color: #fff;
}

.content {
    margin: 4em auto 4em auto;
    background-color: #fff;
    position: relative;
    border: 1px solid #E6E6E6;
}

#press-page .content:before, #sponsor-page .content:before {
    content: "";
    width: 100%;
    height: 10px;
    display: block;
    top: 0;
    position: absolute;
    background-color: #F9A448;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topright: 3px;
    border-top-right-radius: 3px;
    -webkit-border-top-left-radius: 3px;
    -moz-border-radius-topleft: 3px;
    border-top-left-radius: 3px;
}

#showcase-page .content {
    margin: 2em auto;
}

.separator {
    border: 1px solid #ddd;
}

/*
==========================================================================
REGISTRATION
========================================================================== */
#register-page .header-subpage {
    background-color: #AEA3CC;
}

.dropdown div, .register-box p, .register-box span, .register-box input[type="text"] {
    border-bottom: 1px solid #E6E6E6;
}

input.first-name, input.first-name:focus, input.last-name, input.last-name:focus, input.email, input.email:focus, #credit-num, #expir-date {
    border-right: 1px solid #E6E6E6;
}

p.register-date, span.dropdown, #text-total, #total-cost, #expir-date, #mm, #mm span, #yy {
    border-bottom: none;
}

p.last-few {
    background-color: #DD5D5C;
    color: #fff;
    font-weight: bold;
}

.register-box {
    margin: 2em auto 3em auto;
    border: 2px solid #E6E6E6;
    background-color: #fff;
    color: #4D4D4D;
}

.register-box:last-of-type {
    margin-bottom: 0;
}

.register-box form {
    margin: 0;
}

.register-box h2 {
    margin: 0;
    padding: 1.2em 0 0.9em 0;
    font-size: 1em;
    text-align: center;
    -webkit-border-top-left-radius: 3px;
    -moz-border-radius-topleft: 3px;
    border-top-left-radius: 3px;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topright: 3px;
    border-top-right-radius: 3px;
    background-color: #C3B7D6;
    text-transform: uppercase;
    color: #fff;
}

.register-map {
    height: 250px;
}

.register-box p, .register-box span {
    margin: 0;
    padding: 0.9em 1em 0.6em 1em;
}

.register-box span {
    display: inline-block;
    padding: 0.1em 1em 0 1em;
}

.register-box .ticket-type {
    width: 20%;
}

.register-box .ticket-price {
    width: 55%;
    text-align: left;
}

.register-box .dropdown {
    padding: 0;
    width: 25%;
    position: relative;
    top: -2px;
}

.register-box .dropdown::after {
    content: "â–¼";
    padding: 0.2em 0.7em 0 0.7em;
    position: absolute;
    right: -1px;
    pointer-events: none;
    color: #999;
    font-size: 0.625em;
    background-color: #E6E6E6;
}

#ticket-amount {
    height: 3.09em;
    font-size: 1em;
    border-right: none;
    border-top: none;
    position: relative;
}

#ticket-amount:hover {
    cursor: pointer;
}

#text-total, #total-cost {
    width: 25%;
}

#promo-code {
    width: 70%;
    border-top: 1px solid #E6E6E6;
}

#promo-code-button {
    display: inline-block;
    width: 30%;
    height: 3.18em;
    position: relative;
    right: -1px;
    background-color: #A0D388;
    border: 1px solid #A0D388;
    border-left-color: #E6E6E6;
    -webkit-border-bottom-right-radius: 3px;
    -moz-border-radius-bottomright: 3px;
    border-bottom-right-radius: 3px;
    color: #fff;
    text-transform: uppercase;
    -moz-transition: -moz-background 0.2s ease-in-out;
    -o-transition: -o-background 0.2s ease-in-out;
    -webkit-transition: -webkit-background 0.2s ease-in-out;
    transition: background 0.2s ease-in-out;
}

#promo-code-button:hover {
    background-color: #9CCC83;
    -moz-transition: -moz-background 0.2s ease-in-out;
    -o-transition: -o-background 0.2s ease-in-out;
    -webkit-transition: -webkit-background 0.2s ease-in-out;
    transition: background 0.2s ease-in-out;
}

.register-box input[type="text"] {
    height: 3.18em;
    margin: 0;
    padding: 0 1em;
    background-color: transparent;
    display: inline-block;
}

.register-box input[type="text"]:focus {
    background-color: transparent;
}

